{
  "expo": {
    "name": "iResucit√≥",
    "slug": "iresucito",
    "version": "6.0.1",
    "orientation": "portrait",
    "icon": "./img/cristo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./img/bootsplash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*", "@iresucito/core/assets/songs/*.json"],
    "ios": {
      "bundleIdentifier": "com.javiercastro.iresucito"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./img/cristo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.javiercastro.iresucito"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-contacts",
        {
          "contactsPermission": "Permitir $(PRODUCT_NAME) acceso a los contactos."
        }
      ],
      "sentry-expo"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "javier-castro",
            "project": "iresucito"
          }
        }
      ]
    }
  }
}
