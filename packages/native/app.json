{
  "expo": {
    "name": "iResucit√≥",
    "slug": "iresucito",
    "version": "7.0.1",
    "orientation": "default",
    "icon": "./img/cristo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./img/bootsplash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*",
      "../core/assets/**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.javiercastro.iresucito",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSContactsUsageDescription": "Permitir $(PRODUCT_NAME) acceso a los contactos.",
        "UIRequiredDeviceCapabilities": [
          "armv7",
          "gamekit"
        ]
      },
      "buildNumber": "23",
      "requireFullScreen": true,
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "locales": {
      "en": "./languages/english.json",
      "es": "./languages/spanish.json",
      "pt": "./languages/portugues.json",
      "it": "./languages/italian.json",
      "de": "./languages/deutsch.json",
      "fr": "./languages/french.json"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./img/cristo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.javiercastro.iresucito",
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "versionCode": 141
    },
    "plugins": [
      "expo-localization",
      [
        "expo-contacts",
        {
          "contactsPermission": "Permitir $(PRODUCT_NAME) acceso a los contactos."
        }
      ],
      "sentry-expo"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "javier-castro",
            "project": "iresucito"
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "052a0ed0-b388-416d-bd7b-911680ab75d4"
      }
    }
  }
}
