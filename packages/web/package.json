{
  "name": "@iresucito/web",
  "version": "1.0.0",
  "private": true,
  "description": "iResucito webapp",
  "license": "MIT",
  "scripts": {
    "build": "remix setup node && remix build && mkdir -p ./public/build/_assets/themes/default/assets/fonts && cp ./node_modules/semantic-ui-css/themes/default/assets/fonts/* ./public/build/_assets/themes/default/assets/fonts && cp ./node_modules/@iresucito/core/assets/fonts/* ./public/build/_assets/",
    "dev": "node --inspect ./node_modules/.bin/remix dev",
    "start": "node ./syncData.js down && node ./server.js",
    "postinstall": "cp ../../node_modules/pdfjs-dist/build/pdf.worker.js ./public"
  },
  "workspaces": {
    "nohoist": [
      "@iresucito/**",
      "@iresucito/**/**",
      "@remix-run/**",
      "@remix-run/**/**",
      "remix",
      "remix/**",
      "remix-auth",
      "remix-auth/**",
      "remix-auth-form",
      "remix-auth-form/**",
      "semantic-ui-css"
    ]
  },
  "dependencies": {
    "@iresucito/core": "*",
    "@iresucito/translations": "*",
    "@iresucito/scripts": "*",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@remix-run/vercel": "^1.7.0",
    "@types/etag": "^1.8.1",
    "@types/react": "^18.0.17",
    "base64-stream": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "chokidar": "^3.5.3",
    "crypto-random-string": "^5.0.0",
    "diff": "^5.1.0",
    "dropbox": "^10.32.0",
    "etag": "^1.8.1",
    "gmail-send": "^1.8.14",
    "lowdb": "^3.0.0",
    "node-fetch": "^3.2.10",
    "pdfjs-dist": "^2.15.349",
    "plist": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-editor-ajrm": "^2.5.13",
    "react-native-print": "^0.10.0",
    "remix": "^1.7.0",
    "remix-auth": "^3.2.2",
    "remix-auth-form": "^1.1.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3",
    "use-debounce": "^8.0.3",
    "use-hotkeys": "^1.1.0",
    "vercel": "^28.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2"
  }
}
