{
  "name": "@iresucito/core",
  "sideEffects": false,
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build-cjs": "yarn tsc -b tsconfig.cjs.json; echo '{\"type\": \"commonjs\"}' > ./dist/cjs/src/package.json; copyfiles -u 2 ./assets/fonts/* ./dist/cjs/assets/fonts/",
    "build-esm": "yarn tsc -b tsconfig.esm.json; echo '{\"type\": \"module\"}' > ./dist/esm/src/package.json; copyfiles -u 2 ./assets/fonts/* ./dist/esm/assets/fonts/",
    "build-types": "yarn tsc -b tsconfig.types.json",
    "build": "yarn run build-cjs; yarn run build-esm; yarn run build-types",
    "patch": "esno src/applyPatch.ts",
    "test": "esno src/test.ts",
    "test-no-funciona": "// ver comentarios en jest.config.js",
    "postinstall": "yarn run build"
  },
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/types/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    },
    "./generatepdf": {
      "import": {
        "types": "./dist/types/src/generatepdf.d.ts",
        "default": "./dist/esm/src/generatepdf.js"
      },
      "require": {
        "types": "./dist/types/src/generatepdf.d.ts",
        "default": "./dist/cjs/src/generatepdf.js"
      }
    },
    "./assets/*.json": "./dist/cjs/assets/*.json",
    "./assets/songs/*.json": "./dist/cjs/assets/songs/*.json",
    "./assets/fonts/*.ttf": "./assets/fonts/*.ttf"
  },
  "dependencies": {
    "@types/country-list": "^2.1.2",
    "@types/langs": "^2.0.2",
    "@types/lodash": "^4.17.0",
    "@types/pdfkit": "^0.13.4",
    "copyfiles": "^2.4.1",
    "country-list": "^2.3.0",
    "dropbox": "^10.34.0",
    "expo-asset": "~8.10.1",
    "expo-file-system": "~15.4.4",
    "langs": "^2.0.0",
    "lodash": "^4.17.21",
    "normalize-strings": "^1.1.1",
    "pdfkit": "^0.15.0",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "esno": "^0.17.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}
