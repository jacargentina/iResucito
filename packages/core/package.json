{
  "name": "@iresucito/core",
  "sideEffects": false,
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build-cjs": "npx tsc -b tsconfig.cjs.json; echo '{\"type\": \"commonjs\"}' > ./dist/cjs/src/package.json; cp -r ./assets/audios/es ./dist/cjs/assets/audios",
    "build-esm": "npx tsc -b tsconfig.esm.json; echo '{\"type\": \"module\"}' > ./dist/esm/src/package.json; cp -r ./assets/audios/es ./dist/esm/assets/audios",
    "build-types": "npx tsc -b tsconfig.types.json",
    "build": "yarn run build-cjs; yarn run build-esm; yarn run build-types",
    "patch": "esno src/applyPatch.ts",
    "test": "esno src/test.ts",
    "test-no-funciona": "// ver comentarios en jest.config.js"
  },
  "main": "./dist/cjs/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/types/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    },
    "./assets/*.json": "./dist/cjs/assets/*.json",
    "./assets/songs/*.json": "./dist/cjs/assets/songs/*.json",
    "./assets/audios/es/*.mp3": "./dist/cjs/assets/audios/es/*.mp3"
  },
  "dependencies": {
    "@types/country-list": "^2.1.4",
    "@types/langs": "^2.0.5",
    "@types/lodash": "^4.17.5",
    "@types/pdfkit": "^0.13.4",
    "copyfiles": "^2.4.1",
    "country-list": "^2.3.0",
    "dropbox": "^10.34.0",
    "expo-asset": "~9.0.2",
    "expo-file-system": "~16.0.9",
    "langs": "^2.0.0",
    "lodash": "^4.17.21",
    "normalize-strings": "^1.1.1",
    "pdfkit": "^0.15.0",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "esno": "^4.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.5"
  }
}
