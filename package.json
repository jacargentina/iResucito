{
  "name": "iResucito",
  "version": "1.0.0",
  "workspaces": [
    "packages/webapp"
  ],
  "private": true,
  "scripts": {
    "postinstall": "patch-package && cp ./node_modules/pdfjs-dist/build/pdf.worker.js ./packages/webapp/public",
    "start": "react-native start",
    "webpatch": "babel-node ./scripts/applyPatch.js -f ./packages/webapp/data/SongsIndexPatch.json",
    "build": "cd ./webapp && npm run build",
    "web": "cd ./webapp && npm start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 11 Pro'",
    "ios-ipad": "react-native run-ios --simulator='iPad Pro (9.7-inch)'",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "postversion": "react-native-version --never-amend",
    "lint": "eslint . --ext .js",
    "react-devtools": "react-devtools",
    "genpdf": "babel-node ./scripts/songToPdf.js",
    "patch": "node ./packages/webapp/syncData.js down && babel-node ./scripts/applyPatch.js && node ./packages/webapp/syncData.js up SongsIndexPatch.json"
  },
  "jest": {
    "preset": "react-native"
  },
