{
  "name": "iResucito",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "webpatch": "babel-node ./scripts/applyPatch.js -f ./web/data/SongsIndexPatch.json",
    "webclient": "webpack --env.API_PORT=8345 --config ./web/client/webpack.config.js --mode development",
    "webserverdev": "cross-env NODE_ENV=development PORT=8345 babel-node ./web/server/index.js",
    "webserverprod": "cross-env NODE_ENV=production babel-node ./web/server/index.js",
    "website": "yarn run webclient && yarn run webserverdev",
    "build": "webpack --config ./web/client/webpack.config.js --mode production",
    "webprepare": "cd ./web/server && node syncData.js down",
    "web": "yarn run webprepare && yarn run webserverprod",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 11 Pro'",
    "ios-ipad": "react-native run-ios --simulator='iPad Pro (9.7-inch)'",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "postversion": "react-native-version --never-amend",
    "lint": "eslint . --ext .js",
    "react-devtools": "react-devtools",
    "genpdf": "babel-node ./scripts/songToPdf.js",
    "patch": "babel-node ./scripts/applyPatch.js",
    "postinstall": "npx jetify"
  },
